@model SS.Core.Entities.TipoAfectacion

@{
    ViewBag.Title = @SistemaSeguridad.Resources.TipoAfectacionStrings.ModuleName;
}

<h2>@SistemaSeguridad.Resources.TipoAfectacionStrings.ViewDeleteName</h2>

<h3>@SistemaSeguridad.Resources.TipoAfectacionStrings.DeleteConfirm</h3>

@using (Html.BeginForm()) {
<fieldset>

    <table class="style1">
        <tr>
            <th>
                @SistemaSeguridad.Resources.TipoAfectacionStrings.lblName</th>
            <th>
               @SistemaSeguridad.Resources.TipoAfectacionStrings.lblFechaAlta</th>
        </tr>
        <tr>
            <td>
                @Html.DisplayFor(model => model.Nombre)</td>
            <td>
                @Html.DisplayFor(model => model.FechaAlta)</td>
        </tr>
    </table>
</fieldset>

    <p>
        <input type="submit" value=@SistemaSeguridad.Resources.TipoAfectacionStrings.Delete /> |
        @Html.ActionLink(@SistemaSeguridad.Resources.TipoAfectacionStrings.BackToIndex, "Index")
    </p>
    
    @Html.HiddenFor(model => model.Id)
}
